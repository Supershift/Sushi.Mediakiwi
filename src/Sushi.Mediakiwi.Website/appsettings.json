{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MediaKiwi": {
    "ContentService": {
      "ServiceUrl": "https://localhost:5001/api/content/v1.0",
      "ExcludePaths": [ "/_blazor", "/HTML", "/internals" ],
      "SiteFolderPrefix": ""
    },
    "IsDevelopmentMode": true,
    "Locale": "en-US"
  },
  "EnableResponseCaching": false,
  "EnableStaticFileCaching": false,
  "BotUserAgentParts": [
    "Chrome-Lighthouse",
    "Google Page Speed Insights"
  ]
}
